vate func waterlineValue(for session: Session) -> Double {\n   313â†’        var value: Double = 0\n   314â†’        for entry in session.logEntries.sorted(by: { $0.timestamp < $1.timestamp }) {\n   315â†’            if entry.type == .alcohol, let meta = entry.alcoholMeta {\n   316â†’                value += meta.standardDrinkEstimate\n   317â†’            } else if entry.type == .water {\n   318â†’                value -= 1\n   319â†’            }\n   320â†’        }\n   321â†’        return value\n   322â†’    }\n   323â†’\n   324â†’    private func drinkCount(for session: Session) -> Int {\n   325â†’        session.logEntries.filter { $0.type == .alcohol }.count\n   326â†’    }\n   327â†’\n   328â†’    private func waterCount(for session: Session) -> Int {\n   329â†’        session.logEntries.filter { $0.type == .water }.count\n   330â†’    }\n   331â†’\n   332â†’    // MARK: - No Session State\n   333â†’\n   334â†’    private var noSessionContent: some View {\n   335â†’        VStack(spacing: 0) {\n   336â†’            Spacer()\n   337â†’\n   338â†’            startSessionSection\n   339â†’\n   340â†’            Spacer()\n   341â†’\n   342â†’            pastSessionsSection\n   343â†’        }\n   344â†’    }\n   345â†’\n   346â†’    // MARK: - Start Session\n   347â†’\n   348â†’    private var startSessionSection: some View {\n   349â†’        Button {\n   350â†’            startSession()\n   351â†’        } label: {\n   352â†’            Label(\"Start Session\", systemImage: \"play.fill\")\n   353â†’                .font(.headline)\n   354â†’                .frame(maxWidth: .infinity)\n   355â†’                .padding(.vertical, 14)\n   356â†’        }\n   357â†’        .buttonStyle(.borderedProminent)\n   358â†’        .controlSize(.large)\n   359â†’        .padding(.horizontal, 32)\n   360â†’        .padding(.bottom, 32)\n   361â†’    }\n   362â†’\n   363â†’    private func startSession() {\n   364â†’        // Single-active constraint: if one exists, naviga