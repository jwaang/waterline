{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01CdkCXvmXPVkLKyned2aYDy","type":"tool_result","content":"     1â†’import SwiftUI\n     2â†’import SwiftData\n     3â†’import UserNotifications\n     4â†’\n     5â†’struct HomeView: View {\n     6â†’    @Environment(\\.modelContext) private var modelContext\n     7â†’    @Query(\n     8â†’        filter: #Predicate<Session> { !$0.isActive },\n     9â†’        sort: \\Session.startTime,\n    10â†’        order: .reverse\n    11â†’    ) private var pastSessions: [Session]\n    12â†’\n    13â†’    @Query(\n    14â†’        filter: #Predicate<Session> { $0.isActive }\n    15â†’    ) private var activeSessions: [Session]\n    16â†’\n    17â†’    @Query private var users: [User]\n    18â†’\n    19â†’    @State private var navigationPath = NavigationPath()\n    20â†’    @State private var now = Date()\n    21â†’    @State private var showingDrinkSheet = false\n    22â†’\n    23â†’    private var activeSession: Session? { activeSessions.first }\n    24â†’    private var userSettings: UserSettings { users.first?.settings ?? UserSettings() }\n    25â†’    private var warningThreshold: Int { userSettings.warningThreshold }\n    26â†’\n    27â†’    var body: some View {\n    28â†’        NavigationStack(path: $navigationPath) {\n    29â†’            Group {\n    30â†’                if let session = activeSession {\n    31â†’                    activeSessionContent(session)\n    32â†’                } else {\n    33â†’                    noSessionContent\n    34â†’                }\n    35â†’            }\n    36â†’            .navigationTitle(\"Waterline\")\n    37â†’            .toolbar {\n    38â†’                ToolbarItem(placement: .topBarTrailing) {\n    39â†’                    Button {\n    40â†’                        // Settings navigation â€” implemented in US-024\n    41â†’                    } label: {\n    42â†’                        Image(systemName: \"gearshape\")\n    43â†’                    }\n    44â†’                    .accessibilityLabel(\"Settings\")\n    45â†’                }\n    46â†’            }\n    47â†’            .navigationDestination(for: UUID.self) { sessionId in\n    48â†’                sessionDestination(for: sessionId)\n    49â†’            }\n    50â†’            .onReceive(Timer.publish(every: 1, on: .main, in: .common).autoconnect()) { time in\n    51â†’                now = time\n    52â†’            }\n    53â†’        }\n    54â†’    }\n    55â†’\n    56â†’    // MARK: - Active Session State\n    57â†’\n    58â†’    @ViewBuilder\n    59â†’    private func activeSessionContent(_ session: Session) -> some View {\n    60â†’        VStack(spacing: 24) {\n    61â†’            Spacer()\n    62â†’\n    63â†’            WaterlineIndicator(value: waterlineValue(for: session), warningThreshold: warningThreshold)\n    64â†’\n    65â†’            countsSection(for: session)\n    66â†’\n    67â†’            reminderStatusSection(for: session)\n    68â†’\n    69â†’            quickAddButtons(for: session)\n    70â†’\n    71â†’            viewSessionButton(for: session)\n    72â†’\n    73â†’            Spacer()\n    74â†’        }\n    75â†’        .padding(.horizontal, 24)\n    76â†’    }\n    77â†’\n    78â†’    private func countsSection(for session: Session) -> some View {\n    79â†’        HStack(spacing: 32) {\n    80â†’            VStack(spacing: 4) {\n    81â†’                Text(\"\\(drinkCount(for: session))\")\n    82â†’                    .font(.title2.weight(.bold))\n    83â†’                Text(\"Drinks\")\n    84â†’                    .font(.caption)\n    85â†’                    .foregroundStyle(.secondary)\n    86â†’            }\n    87â†’            VStack(spacing: 4) {\n    88â†’                Text(\"\\(waterCount(for: session))\")\n    89â†’                    .font(.title2.weight(.bold))\n    90â†’                Text(\"Water\")\n    91â†’                    .font(.caption)\n    92â†’                    .foregroundStyle(.secondary)\n    93â†’            }\n    94â†’        }\n    95â†’    }\n    96â†’\n    97â†’    // MARK: - Reminder Status\n    98â†’\n    99â†’    private func reminderStatusSection(for session: Session) -> some View {\n   100â†’        VStack(spacing: 8) {\n   101â†’            waterDueText(for: session)\n   102â†’            nextReminderText(for: session)\n   103â†’        }\n   104â†’    }\n   105â†’\n   106â†’    private func waterDueText(for session: Session) -> some View {\n   107â†’        let sinceLastWater = alcoholCountSinceLastWater(for: session)\n   108â†’        let waterEveryN = userSettings.waterEveryNDrinks\n   109â†’        let remaining = max(waterEveryN - sinceLastWater, 0)\n   110â†’\n   111â†’        return HStack(spacing: 4) {\n   112â†’            Image(systemName: \"drop\")\n   113â†’                .foregroundStyle(.blue)\n   114â†’            if remaining == 0 {\n   115â†’                Text(\"Water due now\")\n   116â†’                    .foregroundStyle(.orange)\n   117â†’            } else {\n   118â†’                Text(\"Water due in: \\(remaining) drink\\(remaining == 1 ? \"\" : \"s\")\")\n   119â†’            }\n   120â†’        }\n   121â†’        .font(.subheadline)\n   122â†’        .accessibilityElement(children: .combine)\n   123â†’    }\n   124â†’\n   125â†’    @ViewBuilder\n   126â†’    private func nextReminderText(for session: Session) -> some View {\n   127â†’        if userSettings.timeRemindersEnabled {\n   128â†’            let countdown = nextReminderCountdown(for: session)\n   129â†’            HStack(spacing: 4) {\n   130â†’                Image(systemName: \"bell\")\n   131â†’                    .foregroundStyle(.purple)\n   132â†’                Text(\"Next reminder: \\(countdown)\")\n   133â†’            }\n   134â†’            .font(.subheadline)\n   135â†’            .accessibilityElement(children: .combine)\n   136â†’        }\n   137â†’    }\n   138â†’\n   139â†’    private func alcoholCountSinceLastWater(for session: Session) -> Int {\n   140â†’        var count = 0\n   141â†’        for entry in session.logEntries.sorted(by: { $0.timestamp < $1.timestamp }) {\n   142â†’            if entry.type == .alcohol {\n   143â†’                count += 1\n   144â†’            } else if entry.type == .water {\n   145â†’                count = 0\n   146â†’            }\n   147â†’        }\n   148â†’        return count\n   149â†’    }\n   150â†’\n   151â†’    private func nextReminderCountdown(for session: Session) -> String {\n   152â†’        let intervalSeconds = Double(userSettings.timeReminderIntervalMinutes) * 60\n   153â†’        let sortedEntries = session.logEntries.sorted(by: { $0.timestamp < $1.timestamp })\n   154â†’        let lastLogTime = sortedEntries.last?.timestamp ?? session.startTime\n   155â†’        let nextReminderTime = lastLogTime.addingTimeInterval(intervalSeconds)\n   156â†’        let remaining = nextReminderTime.timeIntervalSince(now)\n   157â†’\n   158â†’        if remaining <= 0 {\n   159â†’            return \"now\"\n   160â†’        }\n   161â†’\n   162â†’        let minutes = Int(remaining) / 60\n   163â†’        let seconds = Int(remaining) % 60\n   164â†’        return String(format: \"%d:%02d\", minutes, seconds)\n   165â†’    }\n   166â†’\n   167â†’    private func quickAddButtons(for session: Session) -> some View {\n   168â†’        HStack(spacing: 16) {\n   169â†’            Button {\n   170â†’                showingDrinkSheet = true\n   171â†’            } label: {\n   172â†’                Label(\"Drink\", systemImage: \"wineglass\")\n   173â†’                    .font(.headline)\n   174â†’                    .frame(maxWidth: .infinity)\n   175â†’                    .padding(.vertical, 14)\n   176â†’            }\n   177â†’            .buttonStyle(.borderedProminent)\n   178â†’            .tint(.orange)\n   179â†’            .accessibilityLabel(\"Add Drink\")\n   180â†’\n   181â†’            Button {\n   182â†’           