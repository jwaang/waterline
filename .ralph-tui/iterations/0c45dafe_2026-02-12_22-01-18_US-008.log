     #expect(settings.defaultWaterAmountOz == 16)\n    45â†’        #expect(settings.units == .ml)\n    46â†’    }\n    47â†’}\n    48â†’\n    49â†’// MARK: - User Model Tests\n    50â†’\n    51â†’@Suite(\"User Model\")\n    52â†’struct UserModelTests {\n    53â†’    @Test(\"User creation with defaults\")\n    54â†’    func createWithDefaults() throws {\n    55â†’        let container = try makeContainer()\n    56â†’        let context = ModelContext(container)\n    57â†’\n    58â†’        let user = User(appleUserId: \"apple-test-id\")\n    59â†’        context.insert(user)\n    60â†’        try context.save()\n    61â†’\n    62â†’        let fetched = try context.fetch(FetchDescriptor<User>())\n    63â†’        #expect(fetched.count == 1)\n    64â†’        #expect(fetched[0].appleUserId == \"apple-test-id\")\n    65â†’        #expect(fetched[0].settings.waterEveryNDrinks == 1)\n    66â†’        #expect(fetched[0].settings.warningThreshold == 2)\n    67â†’        #expect(fetched[0].sessions.isEmpty)\n    68â†’        #expect(fetched[0].presets.isEmpty)\n    69â†’    }\n    70â†’\n    71â†’    @Test(\"User has embedded settings\")\n    72â†’    func embeddedSettings() throws {\n    73â†’        let container = try makeContainer()\n    74â†’        let context = ModelContext(container)\n    75â†’\n    76â†’        var customSettings = UserSettings()\n    77â†’        customSettings.waterEveryNDrinks = 2\n    78â†’        customSettings.units = .ml\n    79â†’        let user = User(appleUserId: \"test\", settings: customSettings)\n    80â†’        context.insert(user)\n    81â†’        try context.save()\n    82â†’\n    83â†’        let fetched = try context.fetch(FetchDescriptor<User>())\n    84â†’        #expect(fetched[0].settings.waterEveryNDrinks == 2)\n    85â†’        #expect(fetched[0].settings.units == .ml)\n    86â†’    }\n    87â†’}\n    88â†’\n    89â†’// MARK: - Session Model Tests\n    90â†’\n    91â†’@Suite(\"Session Model\")\n    92â†’struct SessionMod